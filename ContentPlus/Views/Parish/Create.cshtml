@model ContentPlus.Models.Parish

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

@using (Html.BeginForm("Create", "Parish", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <section class="content-header">
        <h1>
            Parish
            <small>Add Parish</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li><a href="#">Parish</a></li>
            <li class="active">Add Parish</li>
        </ol>
    </section>

     <!-- Main content -->
    <section class="content">
        <div class="row">
            <!-- left column -->
            <div class="col-md-12">
                <!-- general form elements -->
                <div class="box box-primary">
                    <div class="box-header">
                        <h3 class="box-title">Add Parish</h3>
                    </div><!-- /.box-header -->
                    <!-- form start -->
                    <!--<form role="form">-->
                    <div class="box-body">
                        <div class="form-group">
                            <label>Name</label>
                            @Html.TextBoxFor(model => model.Name, new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.Name)
                        </div>
                        <div class="form-group">
                            <label>Estd</label>
                            @Html.TextBoxFor(model => model.Estd, new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.Estd)
                        </div>
                            <div class="form-group">
                                <label>Patron</label>
                                @Html.TextBoxFor(model => model.Patron, new { @class = "form-control" })
                                @Html.ValidationMessageFor(model => model.Patron)
                            </div>

                            <div class="form-group">
                                <label>Diocese</label>
                                @Html.TextBoxFor(model => model.Diocese, new { @class = "form-control" })
                                @Html.ValidationMessageFor(model => model.Diocese)
                            </div>

                            <div class="form-group">
                                <label>Address</label>
                                @Html.TextBoxFor(model => model.Address, new { @class = "form-control" })
                                @Html.ValidationMessageFor(model => model.Address)
                            </div>

                            <div class="form-group">
                                <label>Phone</label>
                                @Html.TextBoxFor(model => model.Phone, new { @class = "form-control" })
                                @Html.ValidationMessageFor(model => model.Phone)
                            </div>
                            <div class="form-group">
                                <label>Email</label>
                                @Html.TextBoxFor(model => model.Email, new { @class = "form-control" })
                                @Html.ValidationMessageFor(model => model.Email)
                            </div>
                            <div class="form-group">
                                <label>Website</label>
                                @Html.TextBoxFor(model => model.Website, new { @class = "form-control" })
                                @Html.ValidationMessageFor(model => model.Website)
                            </div>
                            <div class="form-group">
                                <label>Fbid</label>
                                @Html.TextBoxFor(model => model.Fbid, new { @class = "form-control" })
                                @Html.ValidationMessageFor(model => model.Fbid)
                            </div>
                        <div class="form-group">
                            <label>Vicar</label>
                            @Html.TextBoxFor(model => model.Vicar, new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.Vicar)
                        </div>
                        <div class="form-group">
                            <label>Asstvicar1</label>
                            @Html.TextBoxFor(model => model.Asstvicar1, new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.Asstvicar1)
                        </div>
                        <div class="form-group">
                            <label>Asstvicar2</label>
                            @Html.TextBoxFor(model => model.Asstvicar2, new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.Asstvicar2)
                        </div>
                        <div class="form-group">
                            <label>District</label>
                            @Html.TextBoxFor(model => model.District, new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.District)
                        </div>
                        <div class="form-group">
                            <label>Field1</label>
                            @Html.TextBoxFor(model => model.Field1, new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.Field1)
                        </div>
                        <div class="form-group">
                            <label>Field2</label>
                            @Html.TextBoxFor(model => model.Field2, new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.Field2)
                        </div>
                        <p>
                            <input type="submit" value="Create" class="btn btn-primary" />
                            @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-primary" })
                        </p>
                    </div>
                </div>
            </div>
        </div>
    
    </section><!-- /.content -->
}

<!-- jQuery 2.1.3 -->
<script src="~/styles/plugins/jQuery/jQuery-2.1.3.min.js"></script>
