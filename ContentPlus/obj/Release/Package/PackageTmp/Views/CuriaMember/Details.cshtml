@model ContentPlus.Models.CuriaMember

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Curia Member
        <small>Details</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="#">CuriaMember</a></li>
        <li class="active">Details</li>
    </ol>
</section>

    <section class="content">
    <div class="row">
        <!-- left column -->
        <div class="col-md-12">
            <!-- general form elements -->
            <div class="box box-primary">
                <div class="box-header">
                    <h3 class="box-title">Curia Member Details</h3>
                </div><!-- /.box-header -->
                <!-- form start -->
                <!--<form role="form">-->
                <div class="box-body">
                    <div class="form-group">
                        <label>Curia</label>
                        @Html.DropDownListFor(model => model.CuriaId, new SelectList(ViewBag.CuriaList, "ID", "CuriaName"), new { @class = "form-control" })
                    </div>
                    <div class="form-group">
                        <label>Priest</label>
                        @Html.DropDownListFor(model => model.PriestId, new SelectList(ViewBag.PriestList, "ID", "Name"), new { @class = "form-control" })
                    </div>
                    <div class="form-group">
                        <label>Department</label>
                        @Html.TextBoxFor(model => model.Department, new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.Department)
                    </div>
                    <div class="form-group">
                        <label>Positionheld</label>
                        @Html.TextBoxFor(model => model.Positionheld, new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.Positionheld)

                        <div class="form-group">
                            <label>Status</label>
                            @Html.TextBoxFor(model => model.Status, new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.Status)
                        </div>

                        <div class="form-group">
                            <label>Field1</label>
                            @Html.TextBoxFor(model => model.Field1, new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.Field1)
                        </div>

                        <div class="form-group">
                            <label>Field2</label>
                            @Html.TextBoxFor(model => model.Field2, new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.Field2)
                        </div>

                        <div class="form-group">
                            <label>Field3</label>
                            @Html.TextBoxFor(model => model.Field3, new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.Field3)
                        </div>
                        <p>
                            @Html.ActionLink("Edit", "Edit", new { id = Model.Id }, new { @class = "btn btn-primary" })
                            @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-primary" })
                        </p>
                    </div>
                </div>
            </div>
        </div>
</section><!-- /.content -->
<!-- jQuery 2.1.3 -->
<script src="~/styles/plugins/jQuery/jQuery-2.1.3.min.js"></script>

